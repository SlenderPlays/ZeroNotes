<resources>
    <declare-styleable name="NavButton">
        <attr name="navText" format="string" />
        <attr name="navTextSize" format="dimension" />
        <attr name="navTextColor" format="color" />
        <attr name="navIcon" format="color|reference" />
        <attr name="navIconSize" format="dimension" />
        <attr name="navIconOverscale" format="boolean" />
        <attr name="selectedColor" format="color" />
        <attr name="deselectedColor" format="color" />
    </declare-styleable>
</resources>